<h1>Editing affiliate</h1>

<% form_for(@affiliate) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :org_name %><br />
    <%= f.text_field :org_name %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @affiliate %> |
<%= link_to 'Back', affiliates_path %>